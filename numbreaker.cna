    sub trim {
    
        return tr($1, "\s", "s");
    }
    
    sub long_to_ip {
        local('$num $octet1 $octet2 $octet3 $octet4');
        $num = $1;
        
        $octet1 = ($num >> 24) & 0xFF;
        $octet2 = ($num >> 16) & 0xFF;
        $octet3 = ($num >> 8) & 0xFF;
        $octet4 = $num & 0xFF;
        
        return "$octet1\.$octet2\.$octet3\.$octet4";
    }

