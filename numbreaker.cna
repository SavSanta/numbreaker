    sub calc_cidr {
        local('$res $cidr $ip $prefix $parts $ipNum $mask $network $broadcast $totals');
        
        # Only accept the format "192.168.1.0/24"
        $cidr = $1;
        
        # TODOMAYBE: Pull in th validator routines from ipv4guard for octetted netmasks
        # Split IP and CIDR
        ($ip, $prefix) = split("/", $cidr);
        $prefix = int($prefix);
        
        if ($prefix == $null || $prefix < 0 || $prefix > 32) {
            $res = "ERROR: Invalid CIDR number.";
            return $res;
        }
        
        # Chop octets
        $parts = split("\\.", $ip);
        if (size($parts) != 4) {
            $res = "ERROR: Invalid IPv4 address:". $ip;
            return $res;
        }
        
        # Long Conversions.
        $ipNum = (long($parts[0]) << 24) |
                 (long($parts[1]) << 16) |
                 (long($parts[2]) << 8) |
                 (long($parts[3]));
        
        # Calculate subnet mask (e.g., /24 -> 0xFFFFFF00)
        $mask = -1 << (32 - $prefix);
        
        # Network address: IP AND mask
        $network = $ipNum & $mask;
        
        # Broadcast address: Network OR (inverted mask)
        $broadcast = $network | (0xFFFFFFFFL - $mask);

        $networkAddr = long_to_ip($network);
        $broadcastAddr = long_to_ip($broadcast);
        
        # Calculate total addresses (2^(32-prefix))
        $totals = 1 << (32 - $prefix);
        
        $res .= "\nStart Addr: $networkAddr";
        $res .= "\n\n  End Addr: $broadcastAddr";
        $res .= "\n\n    Total Subnet IPs: $totals";
        
        return $res;
    }
    
